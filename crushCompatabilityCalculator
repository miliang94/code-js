<!DOCTYPE html>
<html>
  <head>
    <title>Crush Compatability Calculator</title>
  </head>
  <body>
    <script>
      prompt("What is your name?");
      prompt("What is their name?");
      var loveScore = Math.random()*100;
      loveScore = Math.floor(loveScore)+1; //number range is from 1 to 100 数字范围是从1到100
      if(loveScore > 70) {
        alert("Your love score is: " + loveScore + "%. You two are a great match!");
      }
      if(loveScore > 30 && loveScore <= 70 ) {
        alert("Your love score is: " + loveScore + "%");
      }
      if(loveScore <= 30 ) {
        alert("Your love score is: " + loveScore + "%. Sorry, you two are not a good match");
      }
    </script>
  </body>
</html>
